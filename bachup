class Normal():
    def nomal(self, doc_name):
        if not os.path.exists("logs"):
            os.mkdir("logs")
        doc = open(doc_name, 'r', encoding='utf8')
        self.name=doc_name + 'NormAlpha.txt'
        fhandw = open(self.name, 'w', encoding='utf8')
        for line in doc:
            # for line in l:
            #     line = re.sub(r"ﺮ", "ر", line)
            line = re.sub(r"ﺮ", "ر", line)
            line = re.sub(r"ﺒ", "ب", line)
            line = re.sub(r"ﺤ", "ح", line)
            line = re.sub(r"ٶ", "و", line)
            line = re.sub(r"ئ", "ی", line)
            line = re.sub(r"ﺏ", "ب", line)
            line = re.sub(r"ﮏ", "ک", line)
            line = re.sub(r"ﻌ", "ع", line)
            line = re.sub(r"ﻴ", "ی", line)
            line = re.sub(r"ﻤ", "م", line)
            line = re.sub(r"ﻆ", "ظ", line)
            line = re.sub(r"ڑ", "ژ", line)
            line = re.sub(r"ﮬ", "ه", line)
            line = re.sub(r"ﻎ", "غ", line)
            line = re.sub(r"ﮥ", "ه", line)
            line = re.sub(r"ݢ", "ک", line)
            line = re.sub(r"ٹ", "ث", line)
