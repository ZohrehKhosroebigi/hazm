class Normal():
    def nomal(self, doc_name):
        if not os.path.exists("logs"):
            os.mkdir("logs")
        doc = open(doc_name, 'r', encoding='utf8')
        self.name=doc_name + 'NormAlpha.txt'
        fhandw = open(self.name, 'w', encoding='utf8')
        for line in doc:
            # for line in l:
            #     line = re.sub(r"ïº®", "Ø±", line)
            line = re.sub(r"ïº®", "Ø±", line)
            line = re.sub(r"ïº’", "Ø¨", line)
            line = re.sub(r"ïº¤", "Ø­", line)
            line = re.sub(r"Ù¶", "Ùˆ", line)
            line = re.sub(r"Ø¦", "ÛŒ", line)
            line = re.sub(r"ïº", "Ø¨", line)
            line = re.sub(r"ï®", "Ú©", line)
            line = re.sub(r"ï»Œ", "Ø¹", line)
            line = re.sub(r"ï»´", "ÛŒ", line)
            line = re.sub(r"ï»¤", "Ù…", line)
            line = re.sub(r"ï»†", "Ø¸", line)
            line = re.sub(r"Ú‘", "Ú˜", line)
            line = re.sub(r"ï®¬", "Ù‡", line)
            line = re.sub(r"ï»", "Øº", line)
            line = re.sub(r"ï®¥", "Ù‡", line)
            line = re.sub(r"İ¢", "Ú©", line)
            line = re.sub(r"Ù¹", "Ø«", line)
            line = re.sub(r"Úˆ", "Ø¯", line)
            line = re.sub(r"Ú†", "Ú©", line)
            line = re.sub(r"ïº‹", "Ø«", line)
            line = re.sub(r"ï»¼", "Ø¯", line)
            line = re.sub(r"ï­™", "Ø¯", line)
            line = re.sub(r"Ùª", "%", line)
            line = re.sub(r"â€¦", "...", line)
            line = re.sub(r"\*", "*", line)
            line = re.sub(r"Ã©", "e", line)
            line = re.sub(r"P", "P", line)
            line = re.sub(r"M", "M", line)
            line = re.sub(r"T", "T", line)
            line = re.sub(r",", ",", line)
            line = re.sub(r"Ù”", "", line)
